{
  "version": "4.1.4",
  "name": "metalsmith-imagemin",
  "description": "Metalsmith plugin to minify images.",
  "author": "Ahmad Nassri <ahmad@ahmadnassri.com> (https://www.ahmadnassri.com/)",
  "homepage": "https://github.com/ahmadnassri/metalsmith-imagemin",
  "repository": {
    "type": "git",
    "url": "https://github.com/ahmadnassri/metalsmith-imagemin.git"
  },
  "license": "ISC",
  "main": "lib/index.js",
  "keywords": [
    "compress",
    "gif",
    "imagemin",
    "images",
    "jpeg",
    "jpg",
    "metalsmith",
    "png"
  ],
  "engines": {
    "node": ">=12"
  },
  "files": [
    "lib"
  ],
  "bugs": {
    "url": "https://github.com/ahmadnassri/metalsmith-imagemin/issues"
  },
  "scripts": {
    "lint:deps": "npx updated",
    "lint:ec": "npx editorconfig-checker .",
    "lint:js": "npx eslint .",
    "lint:md": "npx remark --quiet --frail .",
    "lint": "npx run-p lint:*",
    "open:coverage": "opener coverage/lcov-report/index.html",
    "test": "tap test --coverage-report=lcov --no-browser"
  },
  "devDependencies": {
    "metalsmith": "^2.3.0",
    "rimraf": "^2.6.3",
    "tap": "^12.5.3"
  },
  "dependencies": {
    "imagemin": "^7.0.1",
    "imagemin-advpng": "^6.0.0",
    "imagemin-giflossy": "^5.1.10",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-gm": "^2.0.3",
    "imagemin-jpeg-recompress": "^7.0.0",
    "imagemin-jpegoptim": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-pngcrush": "^7.0.0",
    "imagemin-pngout": "^5.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^9.0.0",
    "imagemin-webp": "^6.0.0",
    "imagemin-zopfli": "^7.0.0"
  }
}
